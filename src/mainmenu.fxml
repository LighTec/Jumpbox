<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MenuController">
  <children>
      <Label prefHeight="51.0" prefWidth="110.0" text="Username" />
      <TextField fx:id="userName" prefHeight="58.0" prefWidth="231.0">
         <VBox.margin>
            <Insets left="100.0" right="100.0" />
         </VBox.margin></TextField>
      <Label prefHeight="51.0" prefWidth="110.0" text="IP of server" />
      <TextField fx:id="ip" prefHeight="58.0" prefWidth="231.0">
         <VBox.margin>
            <Insets left="100.0" right="100.0" />
         </VBox.margin></TextField>
      <Button fx:id="newGameButton" alignment="CENTER" mnemonicParsing="false" prefHeight="39.0" prefWidth="184.0" text="NEW GAME">
         <VBox.margin>
            <Insets top="50.0" />
         </VBox.margin></Button>
      <Button fx:id="joinGameButton"  mnemonicParsing="false" prefHeight="39.0" prefWidth="184.0" text="JOIN GAME">
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </Button>
  </children>
</VBox>
